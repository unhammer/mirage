msgstr "Usage: mirage [OPTION]... FILES|FOLDERS..."

#: mirage.py:645
msgid "Options:"
msgstr "Options:"

#: mirage.py:646
msgid "  -h, --help                   Show this help and exit"
msgstr "  -h, --help                   Affiche cette aide et quitte"

#: mirage.py:647
msgid "  -v, --version                Show version information and exit"
msgstr "  -v, --version                Affiche la version et quitte"

#: mirage.py:648
msgid "  -V, --verbose                Show more detailed information"
msgstr "  -V, --verbose                Affiche plus d'information"

#: mirage.py:649
msgid "  -R, --recursive              Recursively include all images found in"
msgstr "  -R, --recursive              Inclure récursivement les images des"

#: mirage.py:650
msgid "                               subdirectories of FOLDERS"
msgstr "                               sous-répertoires de FOLDERS"

#: mirage.py:651
msgid "  -s, --slideshow              Start in slideshow mode"
msgstr "  -s, --slideshow              Démarrer avec le diaporama"

#: mirage.py:652
msgid "  -f, --fullscreen             Start in fullscreen mode"
msgstr "  -f, --fullscreen             Démarrer en plein écran"

