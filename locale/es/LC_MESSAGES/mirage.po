msgstr "Uso: mirage [OPCION]... FICHEROS|ARCHIVOS..."

#: mirage.py:645
msgid "Options:"
msgstr "Opciones:"

#: mirage.py:646
msgid "  -h, --help                   Show this help and exit"
msgstr "-h ,--help                Ensenar ayuda y cerrar"

#: mirage.py:647
msgid "  -v, --version                Show version information and exit"
msgstr "-v,--version                 Ensenar la informacion de la version y cerrar"

#: mirage.py:648
msgid "  -V, --verbose                Show more detailed information"
msgstr "-V, --verbose                Ensenar informacion más detallada"

#: mirage.py:649
msgid "  -R, --recursive              Recursively include all images found in"
msgstr "-R, --recursive                 Recursivamente incluir todas las imágenes encontradas en"

#: mirage.py:650
msgid "                               subdirectories of FOLDERS"
msgstr "                               subdirectorios de ARCHIVOS"

#: mirage.py:651
msgid "  -s, --slideshow              Start in slideshow mode"
msgstr " -s, --slideshow            Empezar Exposición de Imágenes"

#: mirage.py:652
msgid "  -f, --fullscreen             Start in fullscreen mode"
msgstr " -f, --fullscreen           Empezar en modo pantalla completa"

